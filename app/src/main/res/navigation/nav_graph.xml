<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/universityBillboardFragment2">
    <fragment
        android:id="@+id/universityBillboardFragment2"
        android:name="com.example.universidadtrackerapplication.ui.university.billboard.UniversityBillboardFragment"
        android:label="UniversityBillboardFragment" >
        <action
            android:id="@+id/action_universityBillboardFragment2_to_universityFragment"
            app:destination="@id/universityFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popExitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_universityBillboardFragment2_to_newUniversityFragment"
            app:destination="@id/newUniversityFragment"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            app:popExitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/universityFragment"
        android:name="com.example.universidadtrackerapplication.ui.university.UniversityFragment"
        android:label="fragment_university"
        tools:layout="@layout/fragment_university" />
    <fragment
        android:id="@+id/newUniversityFragment"
        android:name="com.example.universidadtrackerapplication.ui.university.newuniversity.NewUniversityFragment"
        android:label="fragment_new_university"
        tools:layout="@layout/fragment_new_university" />
</navigation>